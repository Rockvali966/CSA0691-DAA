#include <stdio.h>
int main()
{
	int rem=0,temp,n,sum=0;
	printf("enter the number :");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
		rem=n%10;
		sum=sum+(rem*rem*rem);
		n=n/10;
	}
	if(temp==sum)
	{
		printf("the given number is armstorng number");
	}
	else
	{
		printf(" not a armstrong number");
	}
	return 0;
}
